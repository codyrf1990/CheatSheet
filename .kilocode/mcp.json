        "GITHUB_TOOLSETS",
        "-e",
        "GITHUB_READ_ONLY",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "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",
        "GITHUB_TOOLSETS": "",
